{
  "name": "upline.front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "start:pm2": "pm2-runtime start server.ts --name 'upline-frontend'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:react": "vitest --include=tests/**/*.test.tsx",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:react && npm run test:e2e",
    "prepare": "husky",
    "prettier": "prettier --check **/*.tsx",
    "prettier:fix": "prettier --write **/*.tsx",
    "docker:build": "docker-compose -f docker-compose.yml build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@tailwindcss/vite": "^4.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "framer-motion": "^12.7.3",
    "lucide-react": "^0.488.0",
    "pm2": "^5.4.3",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-redux": "^9.2.0",
    "react-router": "^6.29.0",
    "react-router-dom": "^6.29.0",
    "web-vitals": "^4.2.4",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.50.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.12.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.7.0",
    "postcss": "^8.5.1",
    "prettier": "3.4.2",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.4"
  }
}
